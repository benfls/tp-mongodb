{% extends "base.html.twig" %}

{% block title %}Document ""{% endblock %}
{% block content %}
    <p>&larr;&nbsp;<a href="/index.php">Retourner à la liste</a></p>
    {% if entity is empty %}
        <p>Aucune donnée trouvée pour cet ID.</p>
    {% else %}
        <h2>Informations de l'entité</h2>
        <ul>
            {% for key, value in entity %}
                <li><strong>{{ key }}</strong>: {{ value }}</li>
            {% endfor %}
        </ul>
    {% endif %}
{% endblock %}